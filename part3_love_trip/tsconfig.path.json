{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@shared/*": ["src/components/shared/*"],
      "@styles/*": ["src/styles/*"],
      "@pages/*": ["src/pages/*"],
      "@mock/*": ["src/mock/*"],
      "@constants": ["src/constants/index"],
      "@constants/*": ["src/constants/*"],
      "@remote/*": ["src/remote/*"]
    }
  }
}
